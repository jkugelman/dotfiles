[user]
	name = John Kugelman
	email = john@kugelman.name
[color]
	status = auto
	branch = auto
	ui = auto
	interactive = auto
[push]
	default = simple
[alias]
	unstage = reset HEAD --
	lg1 = log --graph --abbrev-commit --decorate --date=relative --format=tformat:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) - %C(dim white)%aN%C(reset)%C(bold yellow)%d%C(reset)'
	lg2 = log --graph --abbrev-commit --decorate --format=tformat:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) - %C(dim white)%aN%C(reset)'
	lg3 = log --graph --abbrev-commit --date=relative --format=tformat:'%C(bold blue)%aN%C(reset), %C(bold green)%ad%C(reset) - %C(auto)%h%d%C(reset)%n''%w(0,4,4)%B%-C()%n'
	lg = !"git lg3"
	pushf = push --force-with-lease --force-if-includes
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[diff "sqlite3"]
	textconv = bash -c 'sqlite3 \"$1\" .dump' --
[pager]
	stash = 
	tag = 
	branch = 
[log]
	mailmap = true
[merge]
	ff = only
[fetch]
	pruneTags = true
	prune = true
[cstk]
	dir = /home/jkugelman/cstk
[cstk "cd"]
	cli = support/cstk-cli
	cl = distros/swfts/Client
	bbs = distros/swfts/Basebuild/build/IA/Basebuild/Setup
	docs = docs
	md = support/make-dist
	docker = support/docker
	website = support/website
	services = services
	ckms = distros/swfts/CM/CKM/src/net/progeny/ia/cm/ckm
	rpm-build = support/docker/rpm-build
[core]
	pager = diff-so-fancy | less -R
[diff-so-fancy]
	stripLeadingSymbols = false
	markEmptyLines = false
[cstk "list"]
	clean = true
[cstk "make"]
	cc = gcc
	os = centos7
[cstk "update"]
	message = true
